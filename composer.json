{
    "name": "firehed/nf",
    "description": "A simple PHP file skeleton generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Stern",
            "email": "eric@ericstern.com"
        }
    ],
    "config": {
      "sort-packages": true
    },
    "require": {
        "symfony/console": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Firehed\\Nf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firehed\\Nf\\": [
                "build"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.5",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-strict-rules": "^0.11.0",
        "phpunit/phpunit": "^8.1",
        "psr/log": "^1.1",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.2",
        "vimeo/psalm": "^3.2"
    }
}
